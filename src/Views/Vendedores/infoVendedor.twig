<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/Project1/src/Public/Css/bootstrap.min.css">
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-2">
                {% include "navbar.twig" %}
            </div>

            <div class="col-10">
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">Nombre</th>
                            <th scope="col">Producto</th>
                            <th scope="col">Ganancia Unitaria</th>
                            <th scope="col">Cantidad</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for data in datos %}
                        <tr>
                            <th scope="row">{{ data['nombre'] }}</th>
                            <th scope="row">{{ data['producto'] }}</th>
                            <th scope="row">{{ data['cantidad'] }}</th>
                            <th scope="row">{{ data['ganancia'] }}</th>
                        </tr>
                        {% endfor %}
        
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>
<script src="/Project1/src/Public/Js/bootstrap.bundle.min.js"></script>
</html>